<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>TwitterTypes</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <imports>
  <import name="desc.type.POS"/>
  </imports>
  <types>
    <typeDescription>
      <name>ude.SocialMediaExplorer.analysis.type.SentimentAnno</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>sentimentValue</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
     <typeDescription>
      <name>ude.SocialMediaExplorer.analysis.type.SenseAnno</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>senseValue</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ude.SocialMediaExplorer.analysis.type.CleanedSenseAnno</name>
      <description>includes the orthographically cleaned senses and their frequenzy in the whole corpus</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>cleanedSense</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>orderByFreqenzy</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>totalFrequenzy</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>ude.SocialMediaExplorer.analysis.type.ArktweetAnno</name>
      <description>links or #s</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>linkOrEmoticon</name>
          <description/>
          <rangeTypeName>de.tudarmstadt.ukp.dkpro.core.api.lexmorph.type.pos.POS</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>ude.SocialMediaExplorer.analysis.type.SimpleSenseAnno</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>simpleSense</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
